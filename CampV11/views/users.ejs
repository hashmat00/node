<% include partials/header %>

<div class='container'>
    
    
    
        
    <div class="row">
    <% users.forEach(function(user){ %>
      <div class="col-md-4">
        <div class="thumbnail">
          <img src="http://1.bp.blogspot.com/--ucL-rXn-Ec/VLwta4arOvI/AAAAAAAABHU/LzjxpJ_cA-g/s1600/wallpaper-for-facebook-profile-photo-738967.jpg" alt="...">
          <div class="caption">
            <h3 align='center'><%= user.username %></h3>
            <!--<p>...</p>-->
            <p align='center'><a  href="/users/<%= user._id %>" class="btn btn-primary btn-lg" role="button">View user</a> </p>
          </div>
        </div>
      </div>
      <% }); %>
      </div>
    
    
    

</div>



